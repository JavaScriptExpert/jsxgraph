<div align="center">{+new Link().toFile("index.html").withText("Class Index")+}
| {+new Link().toFile("files.html").withText("File Index")+}</div>
<hr />
<h2 onclick="document.getElementById('myElList').style.display = ( document.getElementById('myElList').style.display == 'block' ? 'none' : 'block' );">Elements</h2>
<ul class="classList" id="myElList" style="display: block">
	<for each="thisClass" in="data">
	<li>{!
		if(thisClass.isPseudo)
			output += new Link().toClass(thisClass.alias);
	!}</li>
	</for>
</ul>
<h2 onclick="document.getElementById('myClList').style.display = ( document.getElementById('myClList').style.display == 'block' ? 'none' : 'block' );">Classes</h2>
<ul class="classList" id="myClList" style="display: none">
	<for each="thisClass" in="data">
	<li>{!
		if (thisClass.alias == "_global_") {
			output += "<i>"+new Link().toClass(thisClass.alias)+"</i>";
		}
		else {
			if(!thisClass.isPseudo)
				output += new Link().toClass(thisClass.alias);
		}
	!}</li>
	</for>
</ul>
<hr />