</kernel>
<construction title="Eigenschaften der Trigonometrischen Funktionen bsin(x), b 0" author="Spt" date="">
<expression label ="a" exp="a(x) = sin(x)"/>
<element type="function" label="a">
	<show object="true" label="false"/>
	<objColor r="51" g="51" b="255" alpha="0.0"/>
	<animation step="0.1"/>
	<fixed val="false"/>
	<lineStyle thickness="3" type="0"/>
</element><expression label ="b" exp="b(x) = -2 sin(x)"/>
<element type="function" label="b">
	<show object="true" label="false"/>
	<objColor r="0" g="0" b="0" alpha="0.0"/>
	<animation step="0.1"/>
	<fixed val="false"/>
	<lineStyle thickness="2" type="0"/>
</element><expression label ="c" exp="c(x) = -0.5 sin(x)"/>
<element type="function" label="c">
	<show object="true" label="false"/>
	<objColor r="255" g="0" b="51" alpha="0.0"/>
	<animation step="0.1"/>
	<fixed val="false"/>
	<lineStyle thickness="3" type="0"/>
</element></construction>
</geogebra>