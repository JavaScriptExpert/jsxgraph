<?xml version="1.0" encoding="utf-8"?>
<geogebra format="2.3">
<gui>
	<show algebraView="true"/>
	<splitDivider loc="216"/>
	<font  size="13"/>
</gui>
<euclidianView>
	<coordSystem xZero="31.0" yZero="139.0" scale="30.0"/>
	<evSettings axes="true" grid="false" pointCapturing="true"/>
</euclidianView>
<kernel>
	<decimals val="2"/>
